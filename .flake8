[flake8]
max-line-length = 120
exclude = .git,
          .cache,
          .circleci,
          .github,
          .idea,
          .vscode,
          __pycache__,
          config.example.py,
          config.py,
          .env,
          .venv,
          env,
          venv,
          env.bak,
          venv.bak,
          .DS_Store,
          .python-version,
          .tox,
          build,
          dist,
          .pyc,
          alexBot/formatting.py
